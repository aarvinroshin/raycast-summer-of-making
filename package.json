{
	"$schema": "https://www.raycast.com/schemas/extension.json",
	"author": "aarvinroshin",
	"commands": [
		{
			"description": "Set request header cookie",
			"mode": "view",
			"name": "cookie",
			"title": "Cookie"
		},
		{
			"description": "See recent devlogs",
			"mode": "view",
			"name": "devlogs",
			"title": "Devlogs"
		},
		{
			"description": "See latest projects",
			"mode": "view",
			"name": "projects",
			"title": "Projects"
		},
		{
			"description": "See newest users",
			"mode": "view",
			"name": "users",
			"title": "Users"
		},
		{
			"description": "Clear in-memory and persisted caches (keeps cookie)",
			"mode": "no-view",
			"name": "clear-cache",
			"title": "Clear Cache"
		}
	],
	"dependencies": {
		"@raycast/api": "^1.102.3",
		"@raycast/utils": "^2.2.0"
	},
	"description": "Raycast Summer of Making extension",
	"devDependencies": {
		"@types/node": "^24.2.1",
		"@types/react": "^19.1.9",
		"dprint": "^0.50.1",
		"typescript": "^5.9.2"
	},
	"icon": "favicon.png",
	"license": "MIT",
	"name": "summer-of-making",
	"scripts": {
		"build": "ray build",
		"dev": "ray develop",
		"publish": "@raycast/api publish"
	},
	"title": "Summer of Making"
}
